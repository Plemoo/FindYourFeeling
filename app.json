{
  "expo": {
    "name": "FindYourFeeling",
    "slug": "FindYourFeeling",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "jsEngine": "jsc",
      "bundleIdentifier": "com.plemo.FindYourFeeling",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "icon": "assets/images/FeelingIcon.png",
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.plemo.FindYourFeeling"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/splash_FeelingIcon.png",
          "dark": {
            "image": "./assets/images/splash_FeelingIcon.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "assets/fonts/DancingScript-VariableFont_wght.ttf",
            "assets/fonts/Raleway-VariableFont_wght.ttf"
          ]
        }
      ],
      [
      "expo-build-properties",
      {
        "android":{
          "kotlinVersion":"1.9.25"
        }
      }]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f053a34a-d4e6-4b61-be09-f32ae90525c2"
      }
    }
  }
}
