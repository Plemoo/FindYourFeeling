{
  "cli": {
    "version": ">= 16.2.1",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "developmentClient": false,
      "distribution": "store",
      "android": {
        "buildType": "app-bundle",
        "env": {
          "EXPO_KOTLIN_VERSION": "1.9.25"
        }
      },
      "ios": {
        "simulator": false
      }
    },
    "preview": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_KOTLIN_VERSION": "1.9.25"
        }
      },
      "ios": {
        "simulator": true
      }
    }
  }
}